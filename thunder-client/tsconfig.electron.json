{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "outDir": "electron-dist",
    "rootDir": "electron",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["node"],
    "lib": ["ES2021", "DOM"]
  },
  "include": ["electron/**/*.ts"]
}
